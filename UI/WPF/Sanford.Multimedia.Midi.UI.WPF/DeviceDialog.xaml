<Window x:Name="DeviceDialogWindow" x:Class="Sanford.Multimedia.Midi.UI.WPF.DeviceDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sanford.Multimedia.Midi.UI.WPF"
        mc:Ignorable="d"
        Title="MIDI Devices" ResizeMode="NoResize">
    <Grid Height="157" Width="217">
        <Button x:Name="okButton" Content="OK" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="12,122,0,0" Width="75" Height="23" Click="okButton_Click"/>
        <Button x:Name="cancelButton" Content="Cancel" HorizontalAlignment="Left" Margin="123,122,0,0" VerticalAlignment="Top" Width="75" Height="23" Click="cancelButton_Click"/>
        <ComboBox x:Name="inputComboBox" HorizontalAlignment="Left" Margin="12,25,0,0" VerticalAlignment="Top" Width="186"/>
        <ComboBox x:Name="outputComboBox" HorizontalAlignment="Left" Margin="12,72,0,0" VerticalAlignment="Top" Width="186"/>
        <Label x:Name="inputLabel" Content="Input" HorizontalAlignment="Left" Margin="83,2,0,0" VerticalAlignment="Top"/>
        <Label Content="Label" HorizontalAlignment="Left" Margin="83,50,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
